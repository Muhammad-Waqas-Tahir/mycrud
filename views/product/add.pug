extends ../layout

block content
    h1 Add Product
    form(action="/product/add",method="post")
        .form-group
            label(for="name") Product Name
            input#name(type="text",class="form-control" name="Name")
            label(for="Price") Product Price
            input#price(type="text",class="form-control" name="Price")
            label(for="Category") Product Category
            input#price(type="text",class="form-control" name="Category")
            label(for="brand") Product Brand
            input#price(type="text",class="form-control" name="Brand")
        input(type="submit", value="ADD",class="btn btn-dark")    